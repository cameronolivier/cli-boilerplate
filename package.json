{
  "name": "cli-boilerplate",
  "version": "1.1.0",
  "description": "A CLI Boilerplate for quickly scaffolding CLI tools",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "type": "module",
  "keywords": [],
  "author": "Cameron Olivier <cameronolivier@gmail.com> (https://olvr.cc)",
  "bugs": {
    "url": "https://github.com/cameronolivier/cli-boilerplate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cameronolivier/cli-boilerplate.git"
  },
  "license": "MIT",
  "scripts": {
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "build": "tsc",
    "dev": "vitest",
    "test": "vitest run",
    "ci": "npm run build && npm run check-format && npm run check-exports && npm run lint && npm run test",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "npm run ci"
  },
  "dependencies": {
    "chalk": "^5.0.0",
    "commander": "^12.1.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^12.1.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.0",
    "@changesets/cli": "^2.27.9",
    "@types/node": "^22.9.0",
    "eslint": "^9.14.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  }
}
